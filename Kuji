        OrionLib:MakeNotification({
            Name = "Freeze Trade Enabled!",
            Content = "Your pet is now safe... remove all your pets now!",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
    end
})
tradeScamTab:AddTextbox({
    Name = "Username",
    Default = "Player",
    TextDisappear = false,
    Callback = function(Value)
        -- Handle textbox value
    end
})

